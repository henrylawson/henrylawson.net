---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	
	<title type="text" xml:lang="en">{{ site.root_desc }}</title>
	<link type="application/atom+xml" href="http://henrylawson.net/feed/atom.xml" rel="self"/>
 	<link type="text" href="http://henrylawson.net" rel="alternate"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>Henry Hawson (henrylawson.net)</id>
	<author>
		<name>Henry Lawson</name>
	</author>
	<rights>Copyright (c) 2013, Henry Lawson; all rights reserved.</rights>
	
	{% for post in site.posts limit:20 %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://henrylawson.net{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://henrylawson.net{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}
</feed>