---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title type="text" xml:lang="en">{{ site.name }}</title>
  <link type="application/atom+xml" href="{{ site.hostname }}/feed/atom.xml" rel="self"/>
  <link type="text" href="{{ site.hostname }}" rel="alternate"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>Henry Hawson (henrylawson.net)</id>
	<author>
		<name>Henry Lawson</name>
	</author>
  <rights>Copyright (c) 2012 to {{ site.time | date: '%Y' }}, Henry Lawson; all rights reserved.</rights>
  <category term="software"/>
  <logo>{{ site.hostname }}{% asset_path 'henry_lawson.jpg' %}</logo>
  <subtitle>{{ site.meta.description }}</subtitle>
	{% for post in site.posts limit:20 %}
    <entry>
      <category term="software"/>
			<title>{{ post.title }}</title>
      <author>
        <name>Henry Lawson</name>
      </author>
      <link href="{{ site.hostname }}{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
      <id>{{ site.hostname }}{{ post.id }}</id>
      <summary>{{ post.excerpt | strip_html }}</summary>
      <content type="html">{{ post.content | xml_escape }}</content>
      <rights>Copyright (c) 2012 to {{ site.time | date: '%Y' }}, Henry Lawson; all rights reserved.</rights>
		</entry>
	{% endfor %}
</feed>
